--Abaixo eu consigo colocar evento por evento em cada cliente, mas o ideal era inserir o evento 1 para todos os funcionarios --

insert into consta (id_lancamento , id_evento ) values
(1 ,1);

insert into consta (id_lancamento , id_evento ) values
(2 ,1);
 
 
insert into consta (id_lancamento , id_evento ) values
(2 ,2);
 
SELECT * from lancamento; 

select * from eventos; 
 
select * from lancamento; 

select * from consta;

select ev.id_evento, ev.nome_evento, ev.descricao, lanc.id_funcionario, func.nome_funcionario 
from eventos ev
inner join consta cons on cons.id_evento = ev.id_evento
inner join lancamento lanc on lanc.id_lancamento = cons.id_lancamento
inner join funcionario func on func.id_funcionario = lanc.id_funcionario;

