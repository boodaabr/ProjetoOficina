select * from fornecedor;

select * from envia;

insert into envia (id_produto , id_fornecedor )  
(select forn.id_fornecedor , prod.id_produto   from fornecedor forn 
 inner join produto prod on prod.id_produto = 1 and forn.id_fornecedor = 1);
 
