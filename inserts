select lanc.id_lancamento, per.id_periodo, func.nome_funcionario, lanc.total_vantagem, lanc.total_desconto, lanc.total_liquido, per.ano_periodo, per.mes, per.folha, per.status, per.qtd_funcionario, per.hora_final
from periodo per 
inner join por po on po.id_periodo = per.id_periodo
inner join funcionario func on func.id_funcionario = func.id_funcionario
inner join lancamento lanc on func.id_funcionario = lanc.id_funcionario;



