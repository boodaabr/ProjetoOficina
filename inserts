select  prod.id_produto, forn.id_fornecedor, forn.cnpj , forn.razao_social , forn.nome_fantasia, prod.codigo_barras, prod.descricao_produto, prod.fabricante, prod.quantidade,
		prod.preco_custo, prod.preco_venda, prod.montadora , prod.garantia, prod.data_cadastro_produto, prod.categoria 
from envia env
inner join fornecedor forn on forn.id_fornecedor = env.id_fornecedor
inner join produto prod on prod.id_produto = env.id_produto;        

