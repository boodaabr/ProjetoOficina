select * from carro;
-----------------------------------------------------------------------------------------------------------------------------------------------------------------

--Aqui abaixo selecionamos o resultados das tabelas agendamento e carro, passando seus respectivos ids.

select ag.tipo_agendamento, ag.data_agendamento, ag.hora_agendamento, ag.telefone_contato, ag.tipo_veiculo, ag.observacao_agendamento, ca.id_cliente, ca.marca, ca.modelo, ca.placa 
from agendamento ag
inner join carro ca on ag.id_agendamento = 1 and ca.id_carro = 2;

select ag.tipo_agendamento, ag.data_agendamento, ag.hora_agendamento, ag.telefone_contato, ag.tipo_veiculo, ag.observacao_agendamento, ca.id_cliente, ca.marca, ca.modelo, ca.placa 
from agendamento ag
inner join carro ca on ag.id_agendamento = 2 and ca.id_carro =  5;


SELECT * from carro;
-----------------------------------------------------------------------------------------------------------------------------------------------------------------
--Abaixo eu quero saber o resultado de todos os carros que tem agendamento

select ag.tipo_agendamento, ag.data_agendamento, ag.hora_agendamento, ag.telefone_contato, ag.tipo_veiculo, ag.observacao_agendamento ,ag.id_agendamento, rea.id_carro,  ca.marca, ca.modelo, ca.placa
from agendamento ag
inner join realiza rea on ag.id_agendamento = rea.id_agendamento
inner join carro ca on ca.id_carro = rea.id_carro;


