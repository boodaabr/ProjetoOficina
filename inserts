-- Após ter o id_agendamento e o id_carro .. devidamente inserido nas tabelas... inserir na tabela realiza passando os codigos-- 


insert into realiza (id_agendamento ,id_carro )
(select ag.id_agendamento , ca.id_carro from agendamento ag 
inner join carro ca on ag.id_agendamento = 1 and ca.id_carro = 2);

insert into realiza (id_agendamento ,id_carro )
(select ag.id_agendamento , ca.id_carro from agendamento ag 
inner join carro ca on ag.id_agendamento = 2 where ca.id_carro = 5);

